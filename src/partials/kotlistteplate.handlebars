{{#each this}}
<div class="kot-list-container">
    <img src="https://firebasestorage.googleapis.com/v0/b/jonasstasseyns-eindopdracht.appspot.com/o/kot-images%2F{{image}}?alt=media&token=31ae837d-3696-40cb-ad79-c574e60bd790" class="kot-list-img">
    <div class="kot-list-container-content">
        <h1 class="kot-list-title">{{title}}</h1>
        <p class="kot-list-description">{{description}}</p>
        <button class="go-to-detail" id="{{@key}}">Meer info</button>
    </div>
</div>
{{/each}}