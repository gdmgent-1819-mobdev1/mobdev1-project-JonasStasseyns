{{#each kots}}
<div class="kot-list-container">
    <img src="https://firebasestorage.googleapis.com/v0/b/jonasstasseyns-eindopdracht.appspot.com/o/kot-images%2F{{this.image}}?alt=media&token=31ae837d-3696-40cb-ad79-c574e60bd790" class="kot-list-img">
    <h1 class="kot-list-title">{{this.title}}</h1>
    <p class="kot-list-description">{{this.description}}</p>
    <button class="go-to-detail">Meer info</button>
</div>
{{/each}}